    "info_parser": {
        "permanent_slots": [
            ["city","city"],
            ["target_month", "month_slot"],
            ["要社保","shebao_slot"],
            ["要公积金","gongjijin_slot"],
            ["首次","gen_shouci_slot"],
            ["chosen_fee","fee_slot"],
            ["ask_keyi","keyi_slot"],
            ["say_thanks","thanks_slot"],
            ["yourenma","yourenma_slot"]
        ],
        "COMMENT":"ctx slots are always permanent",
        "contextual_slots":[
            ["target_month","ctx_month_slot"],
            ["date","ctx_day_slot"]
        ],
        "slots": {
            "month_slot":{
                "1":["(?<!(十|1))(一|1)月"],
                "2":["(?<!(十|1))(二|2)月"],
                "3":["(三|3)月"],
                "4":["(四|4)月"],
                "5":["(五|5)月"],
                "6":["(六|6)月"],
                "7":["(七|7)月"],
                "8":["(八|8)月"],
                "9":["(九|9)月"],
                "10":["(十|10)月"],
                "11":["(十一|11)月"],
                "12":["(十二|12)月"]
            },
            "fee_slot": {
                "服务费":["服务费|手续费"],
                "首次费":["开户","首次费","没(开|交|缴)过"],
                "社保缴纳":["(五险|社保)(缴纳)*"],
                "社保基数":["社保基数","缴(纳|费)基数"],
                "公积金缴纳":["公积金(缴纳)*","一金"],
                "公积金基数":["公积金基数"],
                "两个":["社保(.*)公积金","五险一金"],
                "一共":["全部|一共|一起|一个月是*多少"]
            },
            "city": {
                "聊城":["聊城"],
                "上海": ["山海","上海","shanghai"],
                "北京": ["北京","beijing"],
                "深圳": ["shenzhen","深圳"],
                "广州": ["广州","guangzhou"],
                "杭州": ["杭州","hangzhou"],
                "成都": ["成都","chengdu"],
                "昆山": ["昆山","kunshan"],
                "苏州": ["苏州","suzhou"]
            },
            "keyi_slot": {
                "asked": [
                    "(能|行|可以)(.*)(么|吗|嚒|吧|不|嘛)",
                    "能不能",
                    "可不可以"
                ],
                "NO_MATCH": ["not_asked"]
            },
            "yourenma_slot":{
                "youma":[
                    "有人(吗|么|嘛|不|没)$"
                ],
                "zaima":[
                    
                    "在(吗|么|嘛|不|没)$"
                ],
                "NO_MATCH":["not_asked"]
            },
            "thanks_slot": {
                "thanked": [
                    "谢了",
                    "(感|谢|真)谢"
                ],
                "NO_MATCH": ["not_thanked"]
            },
            "gen_shouci_slot": {
                "yes": [
                    "没(有*)(交|付|缴|代缴|缴纳)(.*)过",
                    "(?<!不)(.*)首次",
                    "(之|以)前没(有*)(交|付|缴|代缴|缴纳)过"
                ],
                "no": [
                    "(?<!没)(交|付|缴|缴纳)(.+)过",
                    "不(.*)首次",
                    "(之|以)前(有*)(交|付|缴|代缴|缴纳)过"
                ]
            },
            "苏州区_slot": {
                "苏州园区": [
                    "园区"
                ],
                "苏州市区": [
                    "市区"
                ]
            },
            "gongjijin_slot": {
                "yes": [
                    "(?<!(没|不))(要|交|付|缴|代缴|缴纳)公积金",
                    "(?<!(没|不))(和|加)公积金"
                ],
                "no": [
                    "(没|不)(要*)(要|交|付|缴|代缴|缴纳)公积金",
                    "(光|只|单独)(要*)(要|交|付|缴|代缴|缴纳)社保"
                ]
            },
            "shebao_slot":{
                "yes":[
                    "(?<!(没|不))(要|交|付|缴|代缴|缴纳)社保",
                    "(?<!(没|不))(和|加)社保"
                ],
                "no":[
                    "(没|不)(要*)(要|交|付|缴|代缴|缴纳)社保",
                    "(光|只|单独)(要*)(要|交|付|缴|代缴|缴纳)公积金"
                ]
            },
            "default_yes": {
                "NO_MATCH": ["yes"]
            },
            "default_no": {
                "NO_MATCH": ["no"]
            },
            "wantnot": {
                "yes": [
                    "(?<!不)(要|对)",
                    "(?<!不)要*交"
                ],
                "no": [
                    "不要",
                    "(不|没)(交|代缴|缴)"
                ]
            },
            "yesno": {
                "yes": [
                    "yes",
                    "(?<!没)有",
                    "(?<!不)是",
                    "(?<!不)对"
                ],
                "no": [
                    "no",
                    "nope",
                    "不是",
                    "没有",
                    "不$"
                ]
            },
            "ctx_month_slot":{
                "ctx_this_month":[
                    "当月",
                    "现在"
                ],
                "ctx_next_month":[
                    "下(个*)月"
                ]
            },
            "ctx_day_slot":{
                "ctx_today":[
                    "今天",
                    "现在"
                ],
                "ctx_tomorrow":[
                    "明天"
                ]
            }
        }
