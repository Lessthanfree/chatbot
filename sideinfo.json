{
    "conditional_reqs": {
        "city": [
            ["苏州",
                [["苏州区","苏州区_slot"]]
            ],
            ["北京",
                [["北京农城","北京农城_slot"]]
            ]
        ]
    },
    "zones": [
        "city", "work_hrs_flag", "chosen_fee","ss_purchase_cmi_flag", "chosen_service"
    ],
    "secondary_slots": [
        {
            "writeto": "city_district",
            "search_tree": {
                "city": {
                    "上海": "上海市",
                    "北京": {
                        "北京农城":{
                            "北京农业":"北京农业",
                            "北京城镇":"北京城镇"
                        }
                    },
                    "广州": "广州市",
                    "成都": "成都市",
                    "杭州": "杭州市",
                    "深圳": "深圳市",
                    "昆山": "昆山市",
                    "苏州": {
                        "苏州区": {
                            "苏州园区": "苏州园区",
                            "苏州市区": "苏州市区"
                        }
                    }
                }
            }
        },
        {
            "writeto": "ss_purchase_cmi_flag",
            "search_tree": {
                "purchase_type": {
                    "normal": {
                        "w_normal_cmi_flag":{
                            "1.0": "still_can",
                            "0.0": "too_late"
                        }
                    },
                    "bujiao": {
                        "w_bujiao_cmi_flag":{
                            "1.0": "still_can",
                            "0.0": "too_late"
                        }
                    }   
                }
            }
        },
        {
            "writeto": "work_hrs_flag",
            "DEFAULT": "no",
            "search_tree": {
                "state_curr_hour": {
                    "9": "yes",
                    "10": "yes",
                    "11": "yes",
                    "12": "yes",
                    "13": "yes",
                    "14": "yes",
                    "15": "yes",
                    "16": "yes",
                    "17": "yes"
                }
            }
        },
        {
            "writeto": "ss_purchase_for_month",
            "multi":true,
            "search_tree":{
                "ctx_slots.target_month":{
                    "ctx_this_month":["state_month"],
                    "ctx_next_month":["state_month","+",1, 0],
                    "ctx_last_month":["state_month","-",1, 0]
                },
                "target_month":{
                    "_ANY":["target_month"]
                }
            }

        },
        {
            "writeto": "ss_jishu_amount",
            "search_tree":{
                "chosen_service":{
                    "社保":{
                        "ctx_slots.jishu_amt":{
                            "ctx_min":[["city_info",["base_sb_jishu"]]],
                            "ctx_max":[["city_info",["base_sb_jishu_max_limit"]]]
                        }
                    },
                    "公积金":{
                        "ctx_slots.jishu_amt":{
                            "ctx_min":[["city_info",["base_gjj_jishu"]]],
                            "ctx_max":[["city_info",["base_gjj_jishu_max_limit"]]]
                        }
                    }
                },
                "given_amount":{
                    "_ANY":["given_amount"]
                }
            }
        },
        {
            "writeto": "ss_user_spec_component_value",
            "search_tree":{
                "chosen_fee":{
                    "一共": ["shebao_basic_total"],
                    "社保缴纳":["w_shebao_payment"],
                    "社保基数":[["city_info",["sb_jishu"]]],
                    "公积金缴纳":["w_gjj_fee"],
                    "公积金基数":[["city_info",["base_gjj_jishu"]]],
                    "首次费":[["city_info",["shouci_fee"]]],
                    "服务费":["svc_fee_total"]
                }
            }
        }
    ],
    "default_slot_vals": {
        "首次":"yes",
        "要社保":"yes",
        "要公积金":"no",
        "chosen_service":"社保",
        "exclude_svc_fee":"not_mentioned",
        "given_amount": "",
        "purchase_type":"normal"
    },
    "info_parser": {
        "permanent_slots": [
            ["city","city"],
            ["target_month", "month_slot"],
            ["要社保","shebao_slot"],
            ["要公积金","gongjijin_slot"],
            ["首次","gen_shouci_slot"],
            ["chosen_fee","fee_slot"],
            ["exclude_svc_fee", "exclude_svc_fee_slot"],
            ["purchase_type","purchase_type_slot"],
            ["spec_element", "element_slot"]
        ],
        "COMMENT":"ctx slots are also permanent",
        "contextual_slots":[
            ["target_month","ctx_month_slot"],
            ["date","ctx_day_slot"],
            ["jishu_amt", "ctx_minmax_slot"],
            ["ask_keyi","ctx_keyi_slot"],
            ["say_thanks","ctx_thanks_slot"],
            ["yourenma","ctx_yourenma_slot"],
            ["xufei","xufei_slot"]
        ],
        "val_slots":{
            "from_month":{
                "group_pos": 1,
                "map": [
                    "(1?[0-9])月?(~|-|到)(1?[0-9])月?"
                ]
            },
            "to_month" :{
                "group_pos": 2,
                "map":[
                    "(?<=(~|-|到))(1?[0-9])月?"
                ]
            }
        },
        "slots": {
            "duration_slot":{
                "map":{
                    "present":[
                        "(1?[0-9])月?-(1?[0-9])月?",
                        "从?(1?[0-9])月?到(1?[0-9])月?"
                    ],
                    "NO_MATCH":["absent"]
                }
            },
            "month_slot":{
                "multi":true,
                "map":{
                    "1":["(?<!(十|1))(一|1)月"],
                    "2":["(?<!(十|1))(二|2)月"],
                    "3":["(三|3)月"],
                    "4":["(四|4)月"],
                    "5":["(五|5)月"],
                    "6":["(六|6)月"],
                    "7":["(七|7)月"],
                    "8":["(八|8)月"],
                    "9":["(九|9)月"],
                    "10":["(十|10)月"],
                    "11":["(十一|11)月"],
                    "12":["(十二|12)月"]
                }
            },
            "service_slot":{
                "map":{
                    "社保":["五险|社保"],
                    "公积金":["一金|公积金"]
                }
            },
            "fee_slot": {
                "map":{
                    "服务费":["(?<!(算|括|上|加|有))(服务费|手续费)"],
                    "首次费":["开户","首次费","没(开|交|缴)过"],
                    "社保缴纳":["(五险|社保)(缴纳)*"],
                    "社保基数":["社保基数","缴(纳|费)基数"],
                    "公积金缴纳":["公积金(缴纳)?","一金"],
                    "公积金基数":["公积金基数"],
                    "两个":["社保(.*)公积金","五险一金"],
                    "一共":["全部|一共|一起|一个月是*多少"]
                }
            },
            "city": {
                "map":{
                    "聊城":["聊城"],
                    "上海": ["山海","上海","shanghai"],
                    "北京": ["北京","beijing"],
                    "深圳": ["shenzhen","深圳"],
                    "广州": ["广州","guangzhou"],
                    "杭州": ["杭州","hangzhou"],
                    "成都": ["成都","chengdu"],
                    "昆山": ["昆山","kunshan"],
                    "苏州": ["苏州","suzhou"]
                }
            },
            "keyi_slot": {
                "map":{
                    "asked": [
                        "(能|行|可以)(.*)(么|吗|嚒|吧|不|嘛)",
                        "能不能",
                        "可不可以"
                    ],
                    "NO_MATCH": ["not_asked"]
                }
            },
            "yourenma_slot": {
                "map":{
                    "youma":[
                        "有人(吗|么|嘛|不|没)$"
                    ],
                    "zaima":[
                        
                        "在(吗|么|嘛|不|没)$"
                    ],
                    "NO_MATCH":["not_asked"]
                }
            },
            "xufei_slot":{
                "map":{
                    "zhelipaima":[
                        "(还*)(在*)这(里|边)*拍"
                    ],
                    "NO_MATCH":["not_asked"]
                }
            },
            "thanks_slot": {
                "map":{
                    "thanked": [
                        "谢了",
                        "(感|谢|真)谢"
                    ],
                "NO_MATCH": ["not_thanked"]
                }
            },
            "gen_shouci_slot": {
                "map":{
                    "yes": [
                        "没(有*)(交|付|缴|代缴|缴纳)过",
                        "(?<!不)(是*)首次",
                        "(之|以)前没(有*)(交|付|缴|代缴|缴纳)过",
                        "没(有*)开户(过*)"
                    ],
                    "no": [
                        "(?<!没)(交|付|代缴|(?<!代)缴|缴纳)过",
                        "(?<!没)(有?)开户(过?)",
                        "不(是?)首次",
                        "(之|以)前(有*)(交|付|缴|代缴|缴纳)过"
                    ]
                }
            },
            "苏州区_slot": {
                "map":{
                    "苏州园区": [
                        "园区"
                    ],
                    "苏州市区": [
                        "市区"
                    ]
                }
            },
            "gongjijin_slot": {
                "map":{
                    "yes": [
                        "(?<!(没|不))(和|加|要|交|付|缴|代缴|缴纳)?公积金",
                        "(?<!(没|不))一金"
                    ],
                    "no": [
                        "(没|不)(要?)(加|要|交|付|缴|代缴|缴纳)公积金",
                        "(光|只|单独)(要?)(要|交|付|缴|代缴|缴纳)社保"
                    ]
                }
            },
            "shebao_slot":{
                "map":{
                    "yes":[
                        "(?<!(没|不))(要|交|付|缴|代缴|缴纳)社保",
                        "(?<!(没|不))(和|加)社保",
                        "(?<!(没|不))五险"
                    ],
                    "no":[
                        "(没|不)(要?)(要|交|付|缴|代缴|缴纳)社保",
                        "(光|只|单独)(要?)(要|交|付|缴|代缴|缴纳)公积金"
                    ]
                }
            },
            "exclude_svc_fee_slot":{
                "map":{
                    "include":[
                        "(?<!(没|不|别))(算|添加|加上|加|包括)服务费"
                    ],
                    "exclude":[
                        "esf",
                        "除了服务费",
                        "(没|没有|不|不要|别)(算|添加|包括|加上|加)服务费"
                    ]
                }
            },
            "purchase_type_slot":{
                "map":{
                    "normal":["正常"],
                    "bujiao":["补"]
                }
            },
            "service_slot":{
                "社保":["五险|社保"],
                "公积金":["金|公积金"]
            },
            "shebao_slot":{
                "map":{
                    "yes":[
                        "(?<!(没|不))(要|交|付|缴|代缴|缴纳)社保",
                        "(?<!(没|不))(和|加)社保",
                        "(?<!(没|不))五险"
                    ],
                    "no":[
                        "(没|不)(要?)(要|交|付|缴|代缴|缴纳)社保",
                        "(光|只|单独)(要?)(要|交|付|缴|代缴|缴纳)公积金"
                    ]
                }
            },
            "exclude_svc_fee_slot":{
                "map":{
                    "include":[
                        "(?<!(没|不|别))(算|添加|加上|加|包括)服务费"
                    ],
                    "exclude":[
                        "esf",
                        "除了服务费",
                        "(没|没有|不|不要|别)(算|添加|包括|加上|加)服务费"
                    ]
                }
            },
            "北京农城_slot":{
                "map"{
                    "北京农业": [
                        "农村","农业"
                    ],
                    "北京城镇": [
                        "城镇","城市"
                    ]
                }
            },
            "default_yes": {
                "map":{"NO_MATCH": ["yes"]}
            },
            "default_no": {
                "map":{"NO_MATCH": ["no"]}
            },
            "wantnot": {
                "map":{
                    "yes": [
                        "(?<!不)(要|对)"
                    ],
                    "no": []
                }
            },
            "yesno": {
                "map":{
                    "yes": [
                        "yes",
                        "(?<!没)有",
                        "(?<!不)是",
                        "(?<!不)对"
                    ],
                    "no": ["no","nope","不是","没有","不$"]
                }
            },
            "reverse_yesno_slot":{
                "no": [
                    "yes",
                    "(?<!没)有",
                    "(?<!不)是",
                    "(?<!不)对"
                ],
                "yes": [
                    "no",
                    "nope",
                    "不是",
                    "没有",
                    "不$"
                ]
            },
            "DUMMY_SLOT":{
                "map":{
                    "dummy":["abcdef!@#$!$"]
                }
            },
            "ctx_month_slot":{
                "multi": true,
                "map":{
                    "ctx_last_month":[
                        "去月",
                        "上个?月"
                    ],
                    "ctx_this_month":[
                        "当月",
                        "现在",
                        "这(个)?月"
                    ],
                    "ctx_next_month":[
                        "下(个)?月",
                        "明月"
                    ]
                }
            },
            "ctx_day_slot":{
                "map": {
                    "ctx_today": [
                        "今天",
                        "现在"
                    ],
                    "ctx_tomorrow": [
                        "明天"
                    ]
                }
            },
            "ctx_minmax_slot":{
                "map": {
                    "ctx_min": [
                        "最低",
                        "最划算"
                    ],
                    "ctx_max": [
                        "最高",
                        "高额"
                    ]
                }
            }
        }
    },
    "humanizer":{
        "ask_keyi": {
            "asked":{
                "location":"START",
                "text":"可以啊亲。"
            }
        },
        "say_thanks":{
            "thanked":{
                "location":"START",
                "text":"不客气的。"
            }
        },
        "yourenma":{
            "zaima":
            {
                "location":"START",
                "text":"在啊，"
            },
            "youma":
            {
                "location":"START",
                "text":"有啊，"
            }
        },
        "xufei":{
            "zhelipaima":
            {
                "location":"START",
                "text":"对，还在这里拍。"
            }
        }
    }
}