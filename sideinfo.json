{
    "info_parser": {
        "zones": [
            "city"
        ],
        "defaults": [
            ["<COMMENT>", "NOT IMPLEMENTED YET"],
            ["要公积金","no"]
        ],
        "permanent_slots": [
            ["city","city"],
            ["target_month", "month_slot"],
            ["要社保","shebao_slot"],
            ["要公积金","gongjijin_slot"],
            ["首次","gen_shouci_slot"],
            ["ask_keyi","keyi_slot"],
            ["say_thanks","thanks_slot"]
        ],
        "slots": {
            "month_slot":{
                "1":["(?<!(十|1))(一|1)月"],
                "2":["(?<!(十|1))(二|2)月"],
                "3":["(三|3)月"],
                "4":["(四|4)月"],
                "5":["(五|5)月"],
                "6":["(六|6)月"],
                "7":["(七|7)月"],
                "8":["(八|8)月"],
                "9":["(九|9)月"],
                "10":["(十|10)月"],
                "11":["(十一|11)月"],
                "12":["(十二|12)月"]
            },
            "city": {
                "上海": ["山海","上海","shanghai"],
                "北京": [
                    "北京",
                    "beijing"
                ],
                "深圳": [
                    "shenzhen",
                    "深圳"
                ],
                "广州": [
                    "广州",
                    "guangzhou"
                ],
                "杭州": [
                    "杭州",
                    "hangzhou"
                ],
                "成都": [
                    "成都",
                    "chengdu"
                ],
                "昆山": [
                    "昆山"
                ],
                "苏州": [
                    "苏州",
                    "suzhou"
                ]
            },
            "keyi_slot": {
                "可以": [
                    "(能|行|可以)(.*)(么|吗|嚒|吧|不|嘛)",
                    "能不能",
                    "可不可以"
                ],
                "NULL": [
                    "(?<!(可以)).$"
                ]
            },
            "thanks_slot": {
                "谢谢": [
                    "谢了",
                    "(感|谢|真)谢"
                ],
                "NULL": [
                    "(?<!(谢)).$"
                ]
            },
            "gen_shouci_slot": {
                "yes": [
                    "没(有*)(交|付|缴|代缴|缴纳)(.*)过",
                    "(?<!不)(.*)首次",
                    "(之|以)前没(有*)(交|付|缴|代缴|缴纳)过"
                ],
                "no": [
                    "(?<!没)(交|付|缴|缴纳)(.+)过",
                    "不(.*)首次",
                    "(之|以)前(有*)(交|付|缴|代缴|缴纳)过"
                ]
            },
            "苏州区_slot": {
                "苏州园区": [
                    "园区"
                ],
                "苏州市区": [
                    "市区"
                ]
            },
            "gongjijin_slot": {
                "yes": [
                    "(?<!(没|不))(要|交|付|缴|代缴|缴纳)公积金",
                    "(?<!(没|不))(和|加)公积金"
                ],
                "no": [
                    "(没|不)(要*)(要|交|付|缴|代缴|缴纳)公积金",
                    "(光|只|单独)(要*)(要|交|付|缴|代缴|缴纳)社保"
                ]
            },
            "shebao_slot":{
                "yes":[
                    "(?<!(没|不))(要|交|付|缴|代缴|缴纳)社保",
                    "(?<!(没|不))(和|加)社保"
                ],
                "no":[
                    "(没|不)(要*)(要|交|付|缴|代缴|缴纳)社保",
                    "(光|只|单独)(要*)(要|交|付|缴|代缴|缴纳)公积金"
                ]
            },
            "default_yes": {
                "yes": ["."]
            },
            "default_no": {
                "no": ["."]
            },
            "wantnot": {
                "yes": [
                    "(?<!不)(要|对)",
                    "(?<!不)要*交"
                ],
                "no": [
                    "不要",
                    "(不|没)(交|代缴|缴)"
                ]
            },
            "yesno": {
                "yes": [
                    "yes",
                    "(?<!没)有",
                    "(?<!不)是",
                    "(?<!不)对"
                ],
                "no": [
                    "no",
                    "nope",
                    "不是",
                    "没有",
                    "不$"
                ]
            }
        }
    },
    "humanizer":{
        "ask_keyi": {
            "value":"可以",
            "location":"START",
            "text":"可以啊亲。"
        },
        "give_thanks":{
            "value":"谢谢",
            "location":"START",
            "text":"不客气的"
        }
    }
}